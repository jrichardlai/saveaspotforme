<h1>Listing reservations</h1>

<table>
  <tr>
    <th>Remote task</th>
    <th>User</th>
    <th>Yelp</th>
    <th>Restaurant name</th>
    <th>City</th>
    <th>Zip</th>
    <th>Address</th>
    <th>Phone</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reservations.to_a.each do |reservation| %>
  <tr>
    <td><%= reservation.remote_task_id %></td>
    <td><%= reservation.user_id %></td>
    <td><%= reservation.yelp_id %></td>
    <td><%= reservation.location_name %></td>
    <td><%= reservation.city %></td>
    <td><%= reservation.zip %></td>
    <td><%= reservation.address %></td>
    <td><%= reservation.phone %></td>
    <td><%= reservation.latitude %></td>
    <td><%= reservation.longitude %></td>
    <td><%= link_to 'Show', reservation %></td>
    <td><%= link_to 'Edit', edit_reservation_path(reservation) %></td>
    <td><%= link_to 'Destroy', reservation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Reservation', new_reservation_path %>
